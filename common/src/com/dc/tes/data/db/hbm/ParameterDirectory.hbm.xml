<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.dc.tes.data.model">
	<class name="ParameterDirectory" table="parameter_directory">

		<id name="id" column="DIRECTORYID">
			<generator class="identity" />
		</id>

		<property name="systemId" column="SYSTEMID" />
		<property name="parentDirId" column="PARENTDIRID" />
		<property name="sortIndex" column="SORTINDEX" />
		<property name="name" column="NAME" />
		<property name="path" column="PATH" />
		<property name="description" column="DESCRIPTION" />

	</class>
</hibernate-mapping>