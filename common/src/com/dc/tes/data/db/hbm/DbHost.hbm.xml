<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.dc.tes.data.model">
	<class name="DbHost" table="dbhost">

		<id name="hostid" column="HOSTID">
			<generator class="identity" />
		</id>

		<property name="dbHostName" column="DBHOST" />
		<property name="ipaddress" column="IPADDRESS" />
		<property name="portnum" column="PORTNUM" />
		<property name="dbType" column="DBTYPE" />
		<property name="dbName" column="DBNAME" />
		<property name="dbUser" column="DBUSER" />
		<property name="dbPwd" column="DBPWD" />
		<property name="description" column="DESCRIPTION" />
		<property name="systemId" column="SYSTEMID" />
		<property name="isLongConn" column="ISLONGCONN" />
		<property name="osType" column="OSTYPE" />
		
		<property name="createdUserId" column="CreatedUserId" />
		<property name="createdTime" column="CreatedTime" />
		<property name="lastModifiedTime" column="LastModifiedTime" />
		<property name="lastModifiedUserId" column="LastModifiedUserId" />

	</class>
</hibernate-mapping>